.skeleton-animated,
.loading {
  animation: shimmer 1.2s ease-in-out infinite;
  --tag-danger-color: var(--skeleton-color);
  --tag-success-color: var(--skeleton-color);
  --tag-warning-color: var(--skeleton-color);
  --tag-danger-color: var(--skeleton-color);
  --tag-primary-color: var(--skeleton-color);
  --tag-plain-background-color: var(--skeleton-color);
  --step-circle-color: var(--skeleton-color);
  --tag-text-color: transparent !important;
}
.skeleton-animated .skeleton-item,
.skeleton-animated .skeleton-item view,
.skeleton-animated .skeleton-item text,
.loading .skeleton-item,
.loading .skeleton-item view,
.loading .skeleton-item text {
  border-radius: 5px;
  color: transparent !important;
  background-color: var(--skeleton-color);
}
@keyframes shimmer {
50% {
    opacity: 0.6;
}
}
.avatar {
  background-color: #ffbf00;
  vertical-align: middle;
  width: 36px;
  height: 36px;
  line-height: 34px;
  border-radius: 50%;
  color: #fff;
  font-size: 14px;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  margin-right: 8px;
  overflow: hidden;
}
.avatar .small {
  font-size: 12px;
